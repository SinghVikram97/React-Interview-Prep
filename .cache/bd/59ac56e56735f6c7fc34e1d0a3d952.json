:3}},{"generated":{"line":78,"column":0},"source":"Details.js","original":{"line":12,"column":47}}],"sources":{"Details.js":"// We use arrow functions in class based components because\n// Arrow functions don't create context\n// if we use normal function(){} they create a new context\n// so this then refers to something else\n\nimport React, { Component } from \"react\";\n// Can't use hooks with classes\nimport pet from \"@frontendmasters/pet\";\nimport Carousel from \"./Carousel\";\nimport ThemeContext from \"./ThemeContext\";\n\nexport default class Details extends Component {\n  constructor(props) {\n    super(props);\n\n    this.state = {\n      loading: true\n    };\n  }\n\n  componentDidMount() {\n    pet.animal(this.props.id).then(({ animal }) => {\n      this.setState({\n        name: animal.name,\n        animal: animal.type,\n        location: `${animal.contact.address.city}, ${animal.contact.address.state}`,\n        description: animal.description,\n        media: animal.photos,\n        breed: animal.breeds.primary,\n        loading: false\n      });\n    });\n  }\n\n  render() {\n    if (this.state.loading) {\n      return <h1>loading â€¦ </h1>;\n    }\n    const {\n      animal,\n      breed,\n      location,\n      description,\n      media,\n      name,\n      showModal\n    } = this.state;\n\n    return (\n      <div className=\"details\">\n        <Carousel media={media} />\n        <div>\n          <h1>{name}</h1>\n          <h2>{`${animal} â€” ${breed} â€” ${location}`}</h2>\n          <ThemeContext.Consumer>\n            {([theme]) => {\n              return (\n                <button style={{ backgroundColor: theme }}>\n                  {\" \"}\n                  Adopt {name}\n                </button>\n              );\n            }}\n          </ThemeContext.Consumer>\n\n          <p>{description}</p>\n        </div>\n      </div>\n    );\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"5b0a71523c6e09a890f3e0e2b8841d55","cacheData":{"e