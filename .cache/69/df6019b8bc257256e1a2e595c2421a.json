:8}},{"generated":{"line":103,"column":6},"source":"SearchParams.js","original":{"line":31,"column":6}},{"generated":{"line":103,"column":8},"source":"SearchParams.js","original":{"line":69,"column":8}},{"generated":{"line":103,"column":62},"source":"SearchParams.js","original":{"line":31,"column":6}},{"generated":{"line":103,"column":63},"source":"SearchParams.js","original":{"line":30,"column":4}},{"generated":{"line":103,"column":64},"source":"SearchParams.js","original":{"line":29,"column":2}},{"generated":{"line":104,"column":0},"source":"SearchParams.js","original":{"line":73,"column":1}},{"generated":{"line":104,"column":1},"source":"SearchParams.js","original":{"line":3,"column":0}},{"name":"SearchParams","generated":{"line":106,"column":15},"source":"SearchParams.js","original":{"line":75,"column":15}}],"sources":{"SearchParams.js":"import React, { useState, useEffect } from \"react\";\nimport pet, { ANIMALS } from \"@frontendmasters/pet\";\nconst SearchParams = () => {\n  // hooks like useState never go inside if-statements and for-loops\n  // Because hooks keep track of state elements by the order they are called in\n  const [location, setLocation] = useState(\"Seattle, WA\");\n  const [animal, setAnimal] = useState(\"dog\");\n  const [breed, setBreed] = useState(\"\");\n  const [BREEDS, setBREEDS] = useState([]);\n\n  // So everytime they should be called in same order or it messes up things\n\n  // Use effect replaces lifecycle hooks like componentDidMount(), componentWillUnmount()\n  useEffect(() => {\n    setBREEDS([]);\n    setBreed(\"\");\n\n    pet.breeds(animal).then(({ breeds }) => {\n      const breedStrings = breeds.map(({ name }) => name);\n      setBREEDS(breedStrings);\n    });\n  }, [animal]);\n\n  // Declaring dependencies for useEffect so it runs only when animal changes\n  // Otherwise it would run on every re render which would also happen on change of location\n  // If you want to run only once when it renders 1st time pass empty array []\n  // If you want to run everytime something updates don't pass anything\n\n  return (\n    <div className=\"search-params\">\n      <form>\n        <label htmlFor=\"location\">\n          Location\n          <input\n            type=\"text\"\n            id=\"location\"\n            value={location}\n            placeholder=\"Location\"\n            onChange={e => setLocation(e.target.value)}\n          />\n        </label>\n        <label htmlFor=\"animal\">\n          Animal\n          <select\n            id=\"animal\"\n            value={animal}\n            onChange={e => setAnimal(e.target.value)}\n          >\n            <option>All</option>\n            {ANIMALS.map((animal, index) => {\n              return <option key={index}>{animal}</option>;\n            })}\n          </select>\n        </label>\n        <label htmlFor=\"breed\">\n          Breed\n          <select\n            id=\"breed\"\n            value={breed}\n            onChange={e => setBreed(e.target.value)}\n            disabled={!BREEDS.length}\n          >\n            <option>All</option>\n            {BREEDS.map((breed, index) => {\n              return <option key={index}>{breed}</option>;\n            })}\n          </select>\n        </label>\n        <button>Submit</button>\n      </form>\n    </div>\n  );\n};\n\nexport default SearchParams;\n"},"lineCount":null}},"error":null,"hash":"1b12897f8074540ef720244b6fe9a4de","cacheData":{"env":{}}}